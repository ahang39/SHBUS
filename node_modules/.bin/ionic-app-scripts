#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../._@ionic_app-scripts@1.3.0@@ionic/app-scripts/bin/ionic-app-scripts.js" "$@"
  ret=$?
else 
  node  "$basedir/../._@ionic_app-scripts@1.3.0@@ionic/app-scripts/bin/ionic-app-scripts.js" "$@"
  ret=$?
fi
exit $ret
